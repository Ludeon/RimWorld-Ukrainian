  <!-- EN: charge capacitor -->
  <ChargeCapacitor.label>конденсатор заряду</ChargeCapacitor.label>
  <!-- EN: This weapon amplifies pulse-charge technology with a massive capacitor, increasing damage and armor penetration. -->
