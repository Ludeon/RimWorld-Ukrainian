  <CannotPlaceMonumentOnTerrain>Не можна будувати на: {0}.</CannotPlaceMonumentOnTerrain>
  <!-- EN: Cannot place monument over: {0}. -->
  <MonumentOverlapsBuilding>Не можна встановити монумент на: {0}.</MonumentOverlapsBuilding>
