  <MonumentAdjacentToBuilding>Монумент не можна розташувати поруч з: {0}.</MonumentAdjacentToBuilding>
  <!-- EN: Cannot be planted here -->
  <CannotBePlantedHere>Не можна посадити тут</CannotBePlantedHere>
  
  <!-- Mouse attachments -->
  <!-- EN: Expand {0} or\nCreate new {1} -->
  <ExpandOrCreateZone>Розширити {0} або\nСтворити {1}</ExpandOrCreateZone>
